{
    "name": "CindyScript Dark Theme",
    "type": "dark",
    "colors": {
      "editor.foreground": "#D4D4C4",
      "editor.background": "#1a1c20"
    },
    "tokenColors": [
      {
        "scope": "hash.cindyscript",
        "settings": {
          "foreground": "#df6da2"
        }
      },
      {
        "scope": "builtins.cindyscript",
        "settings": {
          "foreground": "#df6da2"
        }
      },
