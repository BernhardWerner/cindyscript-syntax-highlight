      // },
      {
        "scope": "keyword.operator.comparison.cindyscript",
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      {
        "scope": "keyword.operator.assignment.cindyscript",
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      {
        "scope": "string.quoted.double.cindyscript",
        "settings": {
          "foreground": "#74bf7e"
